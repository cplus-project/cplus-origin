include <stdio.h>
include <string.h>

module fmt
module os
module net/http

ot type Student = type {
	in uint8   age
	in uint8   grade
	in uint8   class
	in float32 score
}
ot func Student.init(uint8 age, uint8 grade, uint8 class, float32 score) {
	this.age   = age
	this.grade = grade
	this.class = class
	this.score = score
}

func main() {
	Student std = new Student (17, 2, 12, 612) 
	deal {
		case ERR_WRITE_FAILED:
			error
		case ERR_READ_FAILED:
			error
		default:
			return
	}
	
	std.release(
		id   : myid, 
		name : myname, 
		other: otherinfo
	) 
	deal {
		default: 
			fmt.println("some errors")
	}
}
